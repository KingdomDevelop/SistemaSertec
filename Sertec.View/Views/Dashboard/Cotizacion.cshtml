
@{
    ViewBag.Title = "Cotizacion";
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
}

<div>
    <!-- /col-3 -->
    <div> <!-- / class="col-sm-9"> --> 
        <!-- column 2 -->
        <a href="#"><strong>NUEVA COTIZACION</strong></a>
        <hr />
        <div class="table-responsive" style="width:auto">
            <table class="table table-striped">
                <tr>
                    <th>CALCULO DE PRESUPUESTO</th>
                </tr>
            </table>
            <hr />
            <div class="row">
                <div class="col-md-7">
                    <table class="table table-striped">
                        <tr>
                            <td>OBRA</td>
                            <td>
                                <input type="text" name="obra" id="td_obra" style="width:90%" />
                            </td>
                            <td style="width: 8px"></td>
                        </tr>
                        <tr>
                            <td>FECHA EMISION INFORME</td>
                            <td>
                                <input id="td_fecha" type="datetime-local" name="fecha" class="form-fecha input-md" style="width:90%" value="2018-06-01T08:30">
                            </td>
                            <td style="width: 8px"></td>
                        </tr>
                        <tr>
                            <td>ASCENSOR</td>
                            <td>
                                <input type="text" name="asc" id="td_asc" style="width:90%" class="form-control input-md" />
                            </td>
                            <td style="width: 8px"></td>
                        </tr>
                        <tr>
                            <td>TECNICO EMISOR</td>
                            <td>
                                <input type="text" id="td_tec" list="TecEm"  style="width:90%" class="form-control input-md" />
                                <datalist id="TecEm">
                                    <option value="AGC">
                                    <option value="LLN">
                                    <option value="MGC">
                                    <option value="GLN">
                                    <option value="Otro">
                                </datalist>
                            </td>
                            <td style="width: 8px"></td>
                        </tr>
                        <tr>
                            <td>SUPERVISOR</td>
                            <td>
                                <input type="text" id="td_sup" list="Sup" style="width:90%" class="form-control input-md" />
                                <datalist id="Sup">
                                    <option value="AGC">
                                    <option value="LLN">
                                    <option value="MGC">
                                    <option value="GLN">
                                    <option value="Otro">
                                </datalist>
                            </td>
                            <td style="width: 8px"></td>
                        </tr>
                        <tr>
                            <td>PRESUPUESTO REP N°</td>
                            <td>
                                <input type="text" name="pre" id="td_pre" style="width:90%" class="form-control input-md" />
                            </td>
                            <td style="width: 8px"></td>
                        </tr>
                    </table>
                </div>
                <div class="col-md-4">
                    <table class="table table-striped">
                        <tr>
                            <td>VALOR UF</td>
                            <td><input type="text" name="obra" id="td_uf" style="width:90%" runat="server" /></td>
                            <td>  <asp:TextBox name="obra" id="td_uf_id" runat="server" ViewStateMode="Enabled" Visible="false"></asp:TextBox> </td>
                        </tr>

                        <tr>
                            <td>VALOR HP</td>
                            <td><input type="number" name="hp" id="td_hp" style="width:90%" runat="server" /></td>
                            <td> <asp:TextBox name="hp" id="td_hp_id" runat="server" ViewStateMode="Enabled" Visible="false"></asp:TextBox>  </td>
                        </tr>
                        <tr>
                            <td>VALOR FLETE</td>
                            <td><input type="number" name="flete" id="td_flete" style="width:90%" runat="server" /></td>
                            <td> <asp:TextBox name="ofletebra" id="td_flete_id" runat="server" ViewStateMode="Enabled" Visible="false"></asp:TextBox> </td>

                        </tr>
                        <tr>
                            <td>FECHA CALC</td>
                            <td> <asp:TextBox name="fecha2" id="td_fecha2" runat="server" ViewStateMode="Enabled" style="width:90%;" class="form-control input-md"></asp:TextBox> </td>
                            <td></td>
                        </tr>
                        <tr>
                            <td>PPTO N°</td>
                            <td>
                                <asp:TextBox name="fecha2" id="td_ppto" runat="server" ViewStateMode="Enabled" style="width:90%;" class="form-control input-md"></asp:TextBox>
                            </td>
                            <td></td>
                        </tr>
                    </table>
                </div>
            </div>
            <hr />
            <table  class="table table-striped">
                <tr>
                    <td>BREVE DESCRIPCION DE LA REPARACION A EFECTUAR</td>
                </tr>
                <tr>
                    <td>
                            <textarea id="tf_detalleArea" rows="4" cols="50" style="width:100%" ></textarea>
                    </td>
                </tr>
            </table>
            <hr />
            <div class="table table-striped">
                <table id="TabRepRep" class="table order-list">
                    <thead>
                        <tr>
                            <td>REPUESTOS Y/O REPARACION</td>
                            <td>CANT.</td>
                            <td>CODIGO</td>
                            <td>VALOR UNITARIO</td>
                            <td>SUB TOTAL</td>
                            <td>HP</td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="col-sm-2">
                                <input type="text" name="tfRepuesto" id="tfRepuesto" style="width:90%" class="form-control" />
                            </td>
                            <td class="col-sm-2">
                                <input type="text" name="tfCantidad" id="tfCantidad" style="width:90%" class="form-control" />
                            </td>
                            <td class="col-sm-2">
                                <input type="text" name="tfCodigo" id="tfCodigo" style="width:90%" class="form-control" />
                            </td>
                            <td class="col-sm-2">
                                <input type="number" name="tfValor" id="tfValor" style="width:90%" class="form-control" />
                            </td>
                            <td class="col-sm-2">
                                <input type="number"  name="tfSubtotal" id="tfSubtotal" style="width:90%" class="form-control" />
                            </td>
                            <td class="col-sm-2">
                                <input type="number"  name="tfHp2" id="tfHp2" style="width:90%" class="form-control" />
                            </td>
                        </tr>
                    </tbody>
                    <tfoot>
                        <tr>
                            <td colspan="5" style="text-align: left;">
                                 <input type="button" class="btn btn-success btn-sm" id="Btn_AgregarRep" value="Agregar Repuesto" />
                            </td>
                        </tr>
                    </tfoot>
                </table>
            </div>
            <hr />
            <div class="container" style="width:auto">
                <div class="row" style="overflow-x:scroll; overflow-y:scroll; width:auto">
                    <asp:GridView ID="GridViewAgregarRep"  AutoGenerateColumns="false" EmptyDataText="No records has been added."
                                  CssClass="footable" Width="100%" OnRowDataBound="OnRowDataBound" OnRowEditing="OnRowEditing" OnRowCancelingEdit="OnRowCancelingEdit"
                                  OnRowUpdating="OnRowUpdating" OnRowDeleting="OnRowDeleting">
                        <Columns>
                            <asp:TemplateField HeaderText="REPUESTOS Y/O REPARACION" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblName"  Width="150px" Text='<%# Eval("NameRep") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtName"  Width="150px" Text='<%# Eval("NameRep") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:TemplateField HeaderText="CANTIDAD" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblCantidad"  Width="80px" Text='<%# Eval("Cantidad") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtCantidad"  Width="80px" Text='<%# Eval("Cantidad") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:TemplateField HeaderText="CÓDIGO" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblCodigo" Width="80px" Text='<%# Eval("Codigo") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtCodigo"  Width="80px" Text='<%# Eval("Codigo") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:TemplateField HeaderText="VALOR UNITARIO" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblValor" Width="80px" Text='<%# Eval("Valor") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtValor"  Width="80px" Text='<%# Eval("Valor") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:TemplateField HeaderText="SUB-TOTAL" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblSubTotal"  Width="80px" Text='<%# Eval("SubTotal") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtSubTotal"  Width="80px" Text='<%# Eval("SubTotal") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:TemplateField HeaderText="HP" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblHP"  Width="80px" Text='<%# Eval("HP") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtHP" Width="80px" Text='<%# Eval("HP") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:CommandField ButtonType="Link" ShowEditButton="true" ShowDeleteButton="true" ControlStyle-CssClass="col-6" />
                        </Columns>
                        <SelectedRowStyle BackColor="#CCFFFF" BorderStyle="Solid" Font-Bold="True" />
                    </asp:GridView>
                </div>
            </div>
            <hr />
            <div class="table table-striped">
                <table id="TabTrapTer" class="table table-light">
                    <thead>
                        <tr>
                            <td>TRABAJOS TERCEROS</td>
                            <td>VALOR</td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="col-sm-4">
                                <input type="text" name="tf_terceros" id="tf_terceros" style="width:90%" class="form-control" />
                            </td>
                            <td class="col-sm-4">
                                <input type="text" name="tf_valter" id="tf_valter" style="width:60%" class="form-control" />
                            </td>
                        </tr>
                    </tbody>
                    <tfoot>
                        <tr>
                            <td colspan="2" style="text-align: left;">
                                <input type="button" class="btn btn-success btn-sm" id="Btn_AgregarTrabTer" value="Agregar Trabajo" />
                            </td>
                        </tr>
                    </tfoot>
                </table>
            </div>
            <hr />
            <div class="container" style="width:auto">
                <div class="row" style="overflow-x:scroll; overflow-y:scroll; width:auto">
                    <asp:GridView ID="GridViewAgregarTrabTer" AutoGenerateColumns="false" EmptyDataText="No records has been added."
                                  CssClass="footable" Width="100%" OnRowDataBound="OnRowDataBound" OnRowEditing="OnRowEditing" OnRowCancelingEdit="OnRowCancelingEdit"
                                  OnRowUpdating="OnRowUpdating" OnRowDeleting="OnRowDeleting">
                        <Columns>
                            <asp:TemplateField HeaderText="TRABAJOS TERCEROS" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblTrapTer"  Width="150px" Text='<%# Eval("TrapTer") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtTrapTer" Width="150px" Text='<%# Eval("TrapTer") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:TemplateField HeaderText="VALOR" ControlStyle-CssClass="col-6">
                                <ItemTemplate>
                                    <asp:Label ID="lblValor" server" Width="80px" Text='<%# Eval("Valor") %>'></asp:Label>
                                </ItemTemplate>
                                <EditItemTemplate>
                                    <asp:TextBox ID="txtValor" runat="server" Width="80px" Text='<%# Eval("Valor") %>'></asp:TextBox>
                                </EditItemTemplate>
                            </asp:TemplateField>
                            <asp:CommandField ButtonType="Link" ItemStyle-Width="20px" ShowEditButton="true" ShowDeleteButton="true" ControlStyle-CssClass="col-6" />
                        </Columns>
                    </asp:GridView>
                </div>
            </div>
